{
	"name": "@hyunbinseo/holidays-kr",
	"description": "South Korea's national holidays, based on the official gazette",
	"version": "3.2026.0",
	"scripts": {
		"check": "tsc --noEmit",
		"prepack": "tsup && publint",
		"version": "node --run write && git add -A public",
		"postversion": "git push && git push --tags && node cli/postversion.ts",
		"format": "prettier --write .",
		"write": "node src/write.ts"
	},
	"devDependencies": {
		"@types/node": "^24.7.1",
		"prettier": "^3.6.2",
		"publint": "^0.3.14",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3",
		"valibot": "^1.1.0"
	},
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18"
	},
